<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Patient_Main_Lobby.BatteryLevel_MPP_1_3">


    <com.jjoe64.graphview.GraphView
        android:id="@+id/Graph_BatteryLevel_MPP_1_3"
        id="@+id/graph"
        layout_height="200dip"
        layout_width="match_parent"
        android:layout_width="320dp"
        android:layout_height="271dp"
        android:layout_below="@+id/imageView_BatteryLevel_MPP_1_3"
        android:layout_alignStart="@+id/imageView_BatteryLevel_MPP_1_3"
        android:layout_marginStart="-88dp"
        android:layout_marginTop="30dp" />

    <ImageView
        android:id="@+id/imageView_BatteryLevel_MPP_1_3"
        android:layout_width="144dp"
        android:layout_height="126dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="30dp"
        app:srcCompat="@drawable/battery" />

    <TextView
        android:id="@+id/textView_BatteryLevel"
        android:layout_width="243dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/Graph_BatteryLevel_MPP_1_3"
        android:layout_alignStart="@+id/Graph_BatteryLevel_MPP_1_3"
        android:layout_marginStart="-19dp"
        android:layout_marginTop="64dp"
        android:text="@string/MPP_1_3_textView_BatteryLevel"
        android:textSize="30sp" />

    <TextView
        android:id="@+id/textView_BatteryLevel2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/textView_BatteryLevel"
        android:layout_marginStart="10dp"
        android:layout_marginBottom="0dp"
        android:layout_toEndOf="@+id/textView_BatteryLevel"
        android:text="@string/MPP_1_3_textView_BatteryLevel_CurrentValue"
        android:textSize="30sp" />


</RelativeLayout>